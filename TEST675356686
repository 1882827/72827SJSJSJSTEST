local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/MlFrvWM/BBLT81/main/BL81827TC", true))()

local window = library:AddWindow("Sad | Farming", {
    main_color = Color3.fromRGB(61, 10, 161),
    min_size = Vector2.new(685, 650),
    can_resize = false,
})

local function setFontRecursive(guiObject, font)
    for _, obj in pairs(guiObject:GetDescendants()) do
        if obj:IsA("TextLabel") or obj:IsA("TextButton") or obj:IsA("TextBox") then
            obj.Font = font
        end
    end
end

spawn(function()
    repeat wait() until window.gui
    setFontRecursive(window.gui, Enum.Font.FredokaOne)
end)

local tabFastStrength = window:AddTab("Fast Farming")

tabFastStrength:AddLabel("Farming").TextSize = 25
